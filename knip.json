{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
    "dist/**",
    "coverage/**",
    "gen/**",
    "types/**/*.d.ts",
    "__tests__/**"
  ],
  "ignoreDependencies": [
    "@bufbuild/protoc-gen-es",
    "@cspell/dict-he",
    "@opennextjs/cloudflare",
    "wrangler",
    "eslint-config-next"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": []
}
